import turtle
vroomvroomturtle=turtle.Turtle()
x=10
y=10
for i in range(0,3):
    vroomvroomturtle.forward(x)
    vroomvroomturtle.right(90)
    vroomvroomturtle.forward(x)
    vroomvroomturtle.right(90)
    x=x+y
    vroomvroomturtle.forward(x)
    vroomvroomturtle.right(90)
    vroomvroomturtle.forward(x)
    vroomvroomturtle.right(90)
    x=x+y
    
vroomvroomturtle.forward(x)
vroomvroomturtle.right(90)

turtle.exitonclick()